// Might be replaced with a builtin method
func repeat_str(txt,times){
    var buffer = "";
    var i = 0;
    while i < times {
        buffer += txt;
        i += 1;
    }
    return buffer
}
var i = 0;
loop {
    var wave = (i.sin()*100).round();
    println(repeat_str("#", wave));
    i += 0.015;

}